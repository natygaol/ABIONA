<h1> What kind of trip are you looking for? </h1>


<div class="container">
  <div class="cards">
    <% @travel_styles.each do |style| %>
        <div class="card" style="background-image: url('<%= image_path "#{style.picture}" %>');">
          <p><%= style.title %></p>
        </div>
    <% end %>
  </div>
</div>

<%= link_to "Back", trips_path %>

